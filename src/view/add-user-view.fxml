<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="470.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.AddUserController">
   <children>
      <Label fx:id="add_user_label" layoutX="139.0" layoutY="14.0" text="Add User">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <GridPane alignment="CENTER_RIGHT" layoutX="16.0" layoutY="41.0" nodeOrientation="LEFT_TO_RIGHT" prefHeight="391.0" prefWidth="372.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="155.0" minWidth="10.0" prefWidth="125.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="176.0" minWidth="10.0" prefWidth="143.0" />
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="117.0" minWidth="10.0" prefWidth="95.0" />
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label fx:id="username_label" alignment="CENTER_RIGHT" contentDisplay="RIGHT" text="Username:">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <Label fx:id="password_label" layoutX="10.0" layoutY="31.0" text="Password:" GridPane.rowIndex="1">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <TextField fx:id="username_box" GridPane.columnIndex="1" />
            <TextField fx:id="password_box" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <CheckBox fx:id="admin_check" mnemonicParsing="false" text="Adminstrator" GridPane.columnIndex="1" GridPane.rowIndex="3" />
            <Label fx:id="reenter_pass_label" layoutX="10.0" layoutY="147.0" text="Re-Enter Password:" GridPane.rowIndex="2">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <TextField fx:id="reenter_pass_box" layoutX="119.0" layoutY="145.0" prefWidth="110.0" GridPane.columnIndex="1" GridPane.rowIndex="2" />
         </children>
      </GridPane>
      <Button fx:id="close_button" layoutX="331.0" layoutY="432.0" mnemonicParsing="false" onAction="#onClose" prefHeight="25.0" prefWidth="53.0" text="Close" />
      <Button fx:id="save_button" layoutX="268.0" layoutY="432.0" mnemonicParsing="false" onAction="#onUserSave" prefHeight="25.0" prefWidth="53.0" text="Save" />
   </children>
</AnchorPane>
